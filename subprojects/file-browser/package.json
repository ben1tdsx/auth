  "keywords": [
    "file-browser",
    "login",
    "cookie-session"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1",
    "vite": "^5.0.0"
  }
}
