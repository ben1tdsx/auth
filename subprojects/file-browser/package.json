{
  "name": "file-browser",
  "version": "1.0.0",
  "description": "Web UI for logging in and browsing protected files in node_access_control",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "dev:full": "concurrently --names \"backend,vite\" --prefix-colors \"blue,green\" \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "server": "nodemon server.js"
